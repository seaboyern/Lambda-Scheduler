<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lambda.lambdaorganizer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ScheduleOverview"></activity>
        <activity android:name=".SessionEngineer"></activity>
        <activity android:name=".PomodoroTimer"></activity>
        <activity android:name=".ToDoListManager"></activity>
        <activity android:name=".GradeCalculator"></activity>
    </application>

</manifest>
